    "        # datos = (base, altura)\n",
    "        return datos [0] * datos [1]\n",
    "    elif figura == \"circulo\" :\n",
    "        # datos = (radio,)\n",
    "        return math.pi * datos [0] ** 2\n",
    "    elif figura == \"triangulo\" :\n",
    "        # datos = (base, altura)\n",
    "        return (datos [0] * datos [1]) / 2\n",
    "    else :\n",
    "        return \"Figura no reconocida\"\n",
