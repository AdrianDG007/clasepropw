  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def palabrasQueContienen (listaPalabras, objetivo) :\n",
    "    resultado = []\n",
    "    for palabra in listaPalabras :\n",
    "        if objetivo in palabra :  # comprueba si objetivo esta dentro de la palabra\n",
    "            resultado.append (palabra)\n",
    "    return resultado\n",
    "\n",
    "print ( \"Kata 3 :\", palabrasQueContienen ( [\"casa\", \"castillo\", \"perro\", \"casanova\"], \"casa\" ) )"
